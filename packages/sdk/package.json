{
  "name": "@aegisaudit/sdk",
  "version": "0.2.0",
  "description": "TypeScript SDK for the AEGIS Protocol â€” query and interact with on-chain ZK skill attestations on Base",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "aegis",
    "zk",
    "attestation",
    "ai-agent",
    "skill",
    "base",
    "blockchain",
    "zero-knowledge"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aegisaudit/aegis.git",
    "directory": "packages/sdk"
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@aztec/bb.js": "3.0.0-nightly.20260102",
    "@noir-lang/noir_js": "1.0.0-beta.18",
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.7.0",
    "vitest": "^2.1.0"
  }
}
